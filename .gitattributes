# Auto detect text files and perform LF normalization
* text=auto

# Source code
*.py text eol=lf
*.js text eol=lf
*.ts text eol=lf
*.vue text eol=lf
*.css text eol=lf
*.scss text eol=lf
*.html text eol=lf
*.json text eol=lf
*.xml text eol=lf
*.yaml text eol=lf
*.yml text eol=lf
*.toml text eol=lf
*.md text eol=lf
*.txt text eol=lf
*.sh text eol=lf
*.bat text eol=crlf
*.ps1 text eol=crlf
*.sql text eol=lf
*.po text eol=lf

# Docker
Dockerfile text eol=lf
docker-compose*.yml text eol=lf

# Config
.gitattributes text eol=lf
.gitignore text eol=lf
.editorconfig text eol=lf

